<template name="index$d6d811fa">
  <view class="_div data-v-74c5617b shoppcontainer">
    <view class="_div data-v-74c5617b shoppHeader">
      <view class="_div data-v-74c5617b shoppTitle">
        <view class="_p data-v-74c5617b shoppTitleL">美食推荐</view>
      </view>
    </view>
    <view class="_div data-v-74c5617b shoppList" wx:key="index" key="{{index}}" wx:for="{{newShopp}}" wx:for-index="index" wx:for-item="item">
      <view class="_div data-v-74c5617b shoppListTitle">
        <view class="_div data-v-74c5617b shoppListTitleIcon">店铺</view> {{item.name}} </view>
      <view class="_div data-v-74c5617b shoppListDetail" wx:key="indexList" key="{{indexList}}" wx:for="{{item.shoppingList}}" wx:for-index="indexList" wx:for-item="itemList">
        <view class="_div data-v-74c5617b shoppListDetailImg">
          <image src="{{itemList.shoppingImg}}" alt="{{itemList.shoppingImgAlt}}" class="_img data-v-74c5617b"></image>
        </view>
        <view class="_div data-v-74c5617b shoppListDetailD">
          <view class="_p data-v-74c5617b shoppListDetailDT">{{itemList.shoppListDetailDT}}</view>
          <view class="_p data-v-74c5617b shoppListDetailDP">{{itemList.shoppListDetailDP}}</view>
          <view class="_p data-v-74c5617b shoppListDetailDM">{{itemList.shoppListDetailDM}}</view>
        </view>
      </view>
    </view>
    <view class="_div data-v-74c5617b {{[loadingShow ? 'shoppLoading' : '']}}">
      <i-load-more tip="暂无数据" loading="{{loadingShow}}" class="_i-load-more data-v-74c5617b"></i-load-more>
    </view>
  </view>
</template>