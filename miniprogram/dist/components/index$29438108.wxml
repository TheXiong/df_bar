<template name="index$29438108">
  <view class="_div data-v-9ef0214c commodityContainer">
    <view class="_div data-v-9ef0214c commodityList">点餐</view>
    <view class="_div data-v-9ef0214c commodityListShopp">
      <view class="_div data-v-9ef0214c commodityListShoppMenu">
        <view class="_ul data-v-9ef0214c">
          <view bindtap="handleProxy" wx:key="item.id" key="{{item.id}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{productTypes}}" wx:for-index="index" wx:for-item="item" class="_li data-v-9ef0214c {{[item.id === productTypesId ? 'active' : '']}}">{{item.name}}</view>
        </view>
      </view>
      <view class="_div data-v-9ef0214c commodityListShoppContent">
        <scroll-view scroll-y="true" bindscrolltolower="handleProxy" scroll-top="{{scrollTop}}" bindscroll="handleProxy" class="_scroll-view data-v-9ef0214c commodityToggleList commodityToggle" data-eventid="{{'2'}}" data-comkey="{{$k}}">
          <view bindtap="handleProxy" class="_div data-v-9ef0214c commodityListShoppContentD" wx:key="index" key="{{index}}" id="{{'product'+product.id}}" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}" wx:for="{{productList}}" wx:for-index="index" wx:for-item="product">
            <view class="_div data-v-9ef0214c commodityListShoppContentDImg">
              <image src="{{product.img}}" alt="{{product.alt}}" class="_img data-v-9ef0214c"></image>
            </view>
            <view class="_div data-v-9ef0214c commodityListShoppContentDD">
              <view class="_p data-v-9ef0214c commodityListShoppContentDDT">{{product.name}}</view>
              <view class="_p data-v-9ef0214c commodityListShoppContentDDD">{{product.detail}}</view>
              <view class="_p data-v-9ef0214c commodityListShoppContentDDM">{{product.integral}}元</view>
              <view class="_div data-v-9ef0214c commodityAdd">
                <image src="/static/img/add.png" alt="add" class="_img data-v-9ef0214c"></image>
              </view>
            </view>
          </view>
        </scroll-view>
      </view>
    </view>
    <view class="_div data-v-9ef0214c commodityemtyp"></view>
    <view class="_div data-v-9ef0214c commodityShoppList {{[commodityToggleShow ? 'commodityShoppListToggle' : '']}}">
      <i-cell-group class="_i-cell-group data-v-9ef0214c">
        <i-cell title="{{item.name}}" wx:key="index" key="{{index}}" wx:for="{{commdityShopping}}" wx:for-index="index" wx:for-item="item" class="_i-cell data-v-9ef0214c">
          <view slot="footer" class="_div data-v-9ef0214c">
            <view class="_div data-v-9ef0214c commodityShoppListM">{{item.integral * item.num}}元</view>
            <view class="_div data-v-9ef0214c commodityShoppListSwitch">
              <view class="_div data-v-9ef0214c commodityShoppListSwitchLess" bindtap="handleProxy" data-eventid="{{'3-'+index}}" data-comkey="{{$k}}">－</view>
              <view class="_div data-v-9ef0214c commodityShoppListSwitchSum">{{item.num}}</view>
              <view class="_div data-v-9ef0214c commodityShoppListSwitchAdd" bindtap="handleProxy" data-eventid="{{'4-'+index}}" data-comkey="{{$k}}">＋</view>
            </view>
          </view>
        </i-cell>
        <i-cell title="配送费" class="_i-cell data-v-9ef0214c">
          <view slot="footer" class="_div data-v-9ef0214c">{{commodityPrice >= 10 ? 0 : 1}}元</view>
        </i-cell>
      </i-cell-group>
    </view>
    <view class="_div data-v-9ef0214c commoditySum {{[commoditySumShopp ? 'commoditySumStatus' : '']}}" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">
      <view class="_div data-v-9ef0214c commodityG {{[commoditySumShopp ? 'commodityGStatus' : '']}}">
        <i-icon type="publishgoods_fill" size="40" color="{{ commoditySumShopp === 0 ?  '#5f5f63' : '#ffffff' }}" class="_i-icon data-v-9ef0214c"></i-icon>
        <view wx:if="{{commoditySumShopp}}" class="_div data-v-9ef0214c commodityGsum">{{commoditySumShopp}}</view>
      </view>
      <view class="_div data-v-9ef0214c commodityD">
        <view class="_p data-v-9ef0214c commodityDM {{[commoditySumShopp ? 'commodityDMStatus' : '']}}">{{ commoditySumShopp === 0 ? '未选购商品' : commodityPrice + '元' }}</view>
        <view class="_p data-v-9ef0214c commodityDD">您的积分：{{ userInfo?userInfo.integral:'' }}</view>
      </view>
      <view class="_div data-v-9ef0214c commodityM {{[commoditySumShopp ? 'commodityMStatus' : '']}}" catchtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">{{ commoditySumShopp === 0 ? '' : '去下单'}}</view>
    </view>
    <view bindtap="handleProxy" class="_div data-v-9ef0214c commodityFull {{[commodityToggleShow ? 'commodityFullToggle' : '']}}" data-eventid="{{'7'}}" data-comkey="{{$k}}"></view>
  </view>
</template>