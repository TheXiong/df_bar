<template name="index$442738e8">
  <view class="_div data-v-9ef0214c commodityContainer">
    <view class="_div data-v-9ef0214c commodity">
      <image src="/static/img/cb.png" alt="cb" class="_img data-v-9ef0214c"></image>
      <view class="_div data-v-9ef0214c commodityLogo">
        <image src="/static/img/xbk.png" alt="xbk" class="_img data-v-9ef0214c"></image>
      </view>
    </view>
    <view class="_div data-v-9ef0214c commodityTitle">{{commodityName}}</view>
    <view class="_div data-v-9ef0214c commodityDetail">10元起送|免费配送</view>
    <view class="_div data-v-9ef0214c commodityList"> 点餐 </view>
    <view class="_div data-v-9ef0214c commodityListShopp">
      <view class="_div data-v-9ef0214c commodityListShoppMenu">
        <view class="_ul data-v-9ef0214c">
          <view bindtap="handleProxy" wx:key="index" key="{{index}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{commodityList}}" wx:for-index="index" wx:for-item="item" class="_li data-v-9ef0214c {{[index === commodityIndexs ? 'active' : '']}}">{{item.commodityMenu}}</view>
        </view>
      </view>
      <view class="_div data-v-9ef0214c commodityListShoppContent">
        <view class="_div data-v-9ef0214c commodityToggleList {{[commodityIndex === commodityIndexs ? 'commodityToggle' : '']}}" wx:key="commodityIndex" key="{{commodityIndex}}" wx:for="{{commodityList}}" wx:for-index="commodityIndex" wx:for-item="commodityItem">
          <view bindtap="handleProxy" class="_div data-v-9ef0214c commodityListShoppContentD" wx:key="commodityIndexs" key="{{commodityIndexs}}" data-eventid="{{'1-'+commodityIndex+'-'+commodityIndexs}}" data-comkey="{{$k}}" wx:for="{{commodityItem.commodityData}}"
            wx:for-index="commodityIndexs" wx:for-item="commodityItems">
            <view class="_div data-v-9ef0214c commodityListShoppContentDImg">
              <image src="{{commodityItems.commodityImg}}" alt="{{commodityItems.commodityAlt}}" class="_img data-v-9ef0214c"></image>
            </view>
            <view class="_div data-v-9ef0214c commodityListShoppContentDD">
              <view class="_p data-v-9ef0214c commodityListShoppContentDDT">{{commodityItems.commodityName}}</view>
              <view class="_p data-v-9ef0214c commodityListShoppContentDDD">{{commodityItems.commodityDetail}}</view>
              <view class="_p data-v-9ef0214c commodityListShoppContentDDM">{{commodityItems.commodityMoney}}元</view>
              <view class="_div data-v-9ef0214c commodityAdd">
                <image src="/static/img/add.png" alt="add" class="_img data-v-9ef0214c"></image>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="_div data-v-9ef0214c commodityemtyp"></view>
    <view class="_div data-v-9ef0214c commodityShoppList {{[commodityToggleShow ? 'commodityShoppListToggle' : '']}}">
      <i-cell-group class="_i-cell-group data-v-9ef0214c">
        <i-cell title="{{item.commodityName}}" wx:key="index" key="{{index}}" wx:for="{{commdityShopping}}" wx:for-index="index" wx:for-item="item" class="_i-cell data-v-9ef0214c">
          <view slot="footer" class="_div data-v-9ef0214c">
            <view class="_div data-v-9ef0214c commodityShoppListM">{{item.commodityMoney * item.commoditySum}}元</view>
            <view class="_div data-v-9ef0214c commodityShoppListSwitch">
              <view class="_div data-v-9ef0214c commodityShoppListSwitchLess" bindtap="handleProxy" data-eventid="{{'2-'+index}}" data-comkey="{{$k}}">－</view>
              <view class="_div data-v-9ef0214c commodityShoppListSwitchSum">{{item.commoditySum}}</view>
              <view class="_div data-v-9ef0214c commodityShoppListSwitchAdd" bindtap="handleProxy" data-eventid="{{'3-'+index}}" data-comkey="{{$k}}">＋</view>
            </view>
          </view>
        </i-cell>
        <i-cell title="配送费" class="_i-cell data-v-9ef0214c">
          <view slot="footer" class="_div data-v-9ef0214c"> {{commodityPrice >= 10 ? 0 : 1}}元 </view>
        </i-cell>
      </i-cell-group>
    </view>
    <view class="_div data-v-9ef0214c commoditySum {{[commoditySumShopp ? 'commoditySumStatus' : '']}}" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">
      <view class="_div data-v-9ef0214c commodityG {{[commoditySumShopp ? 'commodityGStatus' : '']}}">
        <i-icon type="publishgoods_fill" size="40" color="{{ commoditySumShopp === 0 ?  '#5f5f63' : '#ffffff' }}" class="_i-icon data-v-9ef0214c"></i-icon>
        <view wx:if="{{commoditySumShopp}}" class="_div data-v-9ef0214c commodityGsum">{{commoditySumShopp}}</view>
      </view>
      <view class="_div data-v-9ef0214c commodityD">
        <view class="_p data-v-9ef0214c commodityDM {{[commoditySumShopp ? 'commodityDMStatus' : '']}}"> {{ commoditySumShopp === 0 ? '未选购商品' : commodityPrice + '元' }} </view>
        <view class="_p data-v-9ef0214c commodityDD"> 不足十元需要加一元配送费 </view>
      </view>
      <view class="_div data-v-9ef0214c commodityM {{[commoditySumShopp ? 'commodityMStatus' : '']}}" catchtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}"> {{ commoditySumShopp === 0 ? '10元起送' : '去结算'}} </view>
    </view>
    <view bindtap="handleProxy" class="_div data-v-9ef0214c commodityFull {{[commodityToggleShow ? 'commodityFullToggle' : '']}}" data-eventid="{{'6'}}" data-comkey="{{$k}}"></view>
  </view>
</template>