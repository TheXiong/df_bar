<template name="index$199a8118">
  <view class="_div data-v-240a3d0a addressAddContainer">
    <view class="_div data-v-240a3d0a addressAddContent">
      <view class="_div data-v-240a3d0a addressAddContentList">
        <view class="_div data-v-240a3d0a addressAddContentListTitle">联系人</view>
        <input class="_input data-v-240a3d0a addressAddContentListInput" type="text" value="{{addressUser.addressName}}" bindinput="handleProxy" autofocus placeholder="名字" data-eventid="{{'0'}}" data-comkey="{{$k}}" />
      </view>
      <view class="_div data-v-240a3d0a addressAddContentList">
        <view class="_div data-v-240a3d0a addressAddContentListTitle">联系电话</view>
        <input class="_input data-v-240a3d0a addressAddContentListInput" type="number" value="{{addressUser.addressPhone}}" bindinput="handleProxy" placeholder="请输入手机号" data-eventid="{{'1'}}" data-comkey="{{$k}}" />
      </view>
      <view class="_div data-v-240a3d0a addressAddContentList">
        <view class="_div data-v-240a3d0a addressAddContentListTitle">详细地址</view>
        <textarea class="_textarea data-v-240a3d0a addressAddContentListInput" value="{{addressUser.addressDetail}}" bindinput="handleProxy" maxlength="30" placeholder="请输入详细地址(最多50字)" data-eventid="{{'2'}}" data-comkey="{{$k}}" />
      </view>
    </view>
    <view wx:if="{{messageStatus}}" class="_p data-v-240a3d0a message">您还没有填完信息哦~</view>
    <view class="_div data-v-240a3d0a operatorButton">
      <view class="_div data-v-240a3d0a addressAddButton del" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">删除</view>
      <view class="_div data-v-240a3d0a addressAddButton save" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">修改</view>
    </view>
    <view class="_div data-v-240a3d0a delContainer" wx:if="{{tipsMessageDel}}">
      <view class="_div data-v-240a3d0a delFull">
        <view class="_div data-v-240a3d0a delContent">
          <view class="_div data-v-240a3d0a delContentTitle"> 确定删除该收货地址吗？ </view>
          <view class="_div data-v-240a3d0a delContentButton">
            <view class="_div data-v-240a3d0a delContentButtonL" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">取消</view>
            <view class="_div data-v-240a3d0a delContentButtonR" bindtap="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}">确定</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>