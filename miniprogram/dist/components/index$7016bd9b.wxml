          <image src="/static/img/qbms.png" alt="qbms" class="_img data-v-50482813"></image>
        </view>
      </view>
    </view>
    <view class="_div data-v-50482813 main">
      <view class="_div data-v-50482813 shop" wx:key="index" key="{{index}}" wx:for="{{merchant}}" wx:for-index="index" wx:for-item="item">
