                <image src="{{itemShoop.shoopingImg}}" alt="{{item.shoopimgAlt}}" class="_img data-v-50482813"></image>
              </view>
              <view class="_p data-v-50482813 shoppingContentTitle">{{itemShoop.shoopingTitle}}</view>
              <view class="_p data-v-50482813 shoppingContentM">
                <label class="_span data-v-50482813 shoppingContentMA">{{itemShoop.shoopingMa}}￥</label>
                <label class="_span data-v-50482813 shoppingContentMB">{{itemShoop.shoopingMb}}</label>
